<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressView">
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeColor" format="color|reference" />
        <attr name="progressColor" format="color|reference" />
        <attr name="progressPercent" format="integer" />
        <attr name="type" format="enum">
            <enum name="ionstone" value="0" />
            <enum name="luwell" value="1" />
        </attr>
    </declare-styleable>
</resources>